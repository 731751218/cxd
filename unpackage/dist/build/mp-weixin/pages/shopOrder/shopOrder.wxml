<view><tab-list vue-id="1" tabList="{{tabList}}" isSelection="{{isSelection}}" data-event-opts="{{[['^tabClick',[['tabClick']]]]}}" bind:tabClick="__e" bind:__l="__l"></tab-list><block wx:if="{{ticketOrder.length}}"><view class="orderlist"><block wx:for="{{ticketOrder}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="order"><view class="top"><view>{{''+item.goods_name+''}}</view><block wx:if="{{item.state=='已支付'}}"><view class="green">{{''+item.state+''}}</view></block><block wx:if="{{item.state=='已领取'}}"><view class="blue">{{''+item.state+''}}</view></block><block wx:if="{{item.state=='已失效'}}"><view class="red">{{''+item.state+''}}</view></block></view><view class="bottom"><view class="price">{{'金额:'+(item.price=='0.00'?'免费':item.price)+''}}</view><view>{{item.time}}</view></view><view class="receive"><view class="number">{{''+item.number+''}}</view><block wx:if="{{item.state=='已支付'}}"><view data-event-opts="{{[['tap',[['receiveTicket',['$0','$1'],[[['ticketOrder','',index,'card_ticket_id']],[['ticketOrder','',index,'id']]]]]]]}}" class="btn1" bindtap="__e">立即领取</view></block><block wx:if="{{item.state=='已领取'}}"><view data-event-opts="{{[['tap',[['noReceive',['该卡券已经领取过了!']]]]]}}" class="btn2" bindtap="__e">已领取</view></block><block wx:if="{{item.state=='已失效'}}"><view data-event-opts="{{[['tap',[['noReceive',['该卡券已经失效!']]]]]}}" class="btn2" bindtap="__e">已失效</view></block></view></view></block></view></block><block wx:else><nodata vue-id="2" height="800" text="暂无提现记录" bind:__l="__l"></nodata></block></view>
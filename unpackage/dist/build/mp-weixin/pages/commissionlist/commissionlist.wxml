<view><view class="content"><block wx:if="{{commissionlist.length}}"><view class="list"><block wx:for="{{commissionlist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item"><view><view class="header"><view class="img"><image src="{{item.showinfo.avatarurl}}"></image></view><view class="username">{{''+item.showinfo.nickname+''}}</view></view><view>{{''+item.valuechange+'元'}}</view></view><view><view class="time">{{item.time}}</view><block wx:if="{{item.type=='系统操作'||item.type=='下级返佣'}}"><view class="commissiontype" style="color:#007AFF;">{{item.type}}</view></block><block wx:else><view class="commissiontype" style="color:#f00;">{{item.type}}</view></block></view></view></block></view></block><block wx:else><nodata vue-id="1" height="1000" text="暂无佣金记录" bind:__l="__l"></nodata></block></view></view>
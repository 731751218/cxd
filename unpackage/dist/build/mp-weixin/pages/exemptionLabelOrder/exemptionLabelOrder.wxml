<view><tab-list vue-id="1" tabList="{{tabList}}" isSelection="{{isSelection}}" data-event-opts="{{[['^tabClick',[['tabClick']]]]}}" bind:tabClick="__e" bind:__l="__l"></tab-list><block wx:if="{{exemptionLabelOrderList.length}}"><view class="list"><block wx:for="{{exemptionLabelOrderList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item"><view class="header"><view>{{''+item.number+''}}</view><block wx:if="{{item.status=='已付款'}}"><view class="state suc">{{''+item.status+''}}</view></block><block wx:if="{{item.status=='已发货'}}"><view class="state wan">{{''+item.status+''}}</view></block><block wx:if="{{item.status=='已签收'}}"><view class="state blu">{{''+item.status+''}}</view></block><block wx:if="{{item.status=='已驳回'}}"><view class="state red">{{''+item.status+''}}</view></block></view><view class="body"><view class="carnumber">{{'车牌号: '+item.carnumber+''}}</view><view class="price">{{'¥'+item.price+''}}</view></view><view class="footer"><view class="time">{{''+item.time+''}}</view><view class="details" hover-class="detailshover" data-event-opts="{{[['tap',[['details',['$0'],[[['exemptionLabelOrderList','',index,'id']]]]]]]}}" bindtap="__e">查看详情</view></view></view></block></view></block><block wx:else><nodata vue-id="2" height="{{1000}}" text="暂无免检标订单" bind:__l="__l"></nodata></block></view>
<view><tab-list vue-id="1" tabList="{{tabList}}" isSelection="{{isSelection}}" data-event-opts="{{[['^tabClick',[['tabClick']]]]}}" bind:tabClick="__e" bind:__l="__l"></tab-list><block wx:if="{{testingOrderList.length}}"><view class="list"><block wx:for="{{testingOrderList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item"><view class="row"><view class="left">审车类型</view><block wx:if="{{item.isreplace=='预约审车'}}"><view class="right suc">{{item.isreplace}}</view></block><block wx:else><view class="right blu">{{item.isreplace}}</view></block></view><view class="row"><view class="left">{{item.carnumber}}</view><view class="right">{{item.price}}</view></view><view class="row"><view class="left">{{"订单编号:"+item.number}}</view></view><view class="row"><block wx:if="{{item.state=='支付成功'}}"><view class="left suc">{{item.state}}</view></block><block wx:if="{{item.state=='订单受理'}}"><view class="left err">{{item.state}}</view></block><block wx:if="{{item.state=='订单完成'}}"><view class="left yel">{{item.state}}</view></block><block wx:if="{{item.state=='订单退回'}}"><view class="left err">{{item.state}}</view></block><view class="right btn" hover-class="btns" data-event-opts="{{[['tap',[['onOrderDetails',['$0'],[[['testingOrderList','',index,'id']]]]]]]}}" bindtap="__e">查看订单详情</view></view></view></block></view></block><block wx:else><nodata vue-id="2" height="800" text="暂无提现记录" bind:__l="__l"></nodata></block></view>
.itemlist {
  background-color: #fff;
  margin-top: 30rpx;
  border-top: 1rpx solid #DEDEDE;
  border-bottom: 1rpx solid #DEDEDE;
}
.itemlist .item {
    background-color: #FFF;
    padding: 20rpx 30rpx;
    box-sizing: border-box;
    font-size: 34rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: start;
    -webkit-justify-content: flex-start;
            justify-content: flex-start;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -webkit-flex-direction: row;
            flex-direction: row;
}
.itemlist .item .left {
      width: 30%;
}
.itemlist .item .right {
      width: 70%;
      text-align: right;
}
.itemlist .drivinglicensewrap {
    margin-bottom: 30rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;

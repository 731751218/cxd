            flex-direction: row;
    height: 220rpx;
}
.itemlist .drivinglicensewrap .items {
      width: 30%;
      height: 100%;
      border: 1rpx solid #dedede;
      border-radius: 10rpx;
      overflow: hidden;
}
.itemlist .drivinglicensewrap .items .img {
        width: 100%;
        height: 160rpx;
        background-color: #fff;
        position: relative;
}
.itemlist .drivinglicensewrap .items .img .close {
          position: absolute;
          top: 0;
          right: 0;
          width: 40rpx;
          height: 40rpx;
          background-color: #f00;
          color: #fff;
}
.itemlist .drivinglicensewrap .items .img > view {
          color: #999;
          height: 100%;
          padding: 20rpx 0;
          display: -webkit-box;
          display: -webkit-flex;
          display: flex;

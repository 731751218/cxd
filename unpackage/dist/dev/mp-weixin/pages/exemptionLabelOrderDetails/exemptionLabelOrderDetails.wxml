<view><view class="item"><view class="left">姓名</view><view class="right">{{exemptionLabelOrderDetails.name}}</view></view><view class="item"><view class="left">电话</view><view class="right">{{exemptionLabelOrderDetails.phone}}</view></view><view class="item"><view class="left">车牌号</view><view class="right">{{exemptionLabelOrderDetails.carnumber}}</view></view><view class="item"><view class="left">支付金额</view><view class="right">{{exemptionLabelOrderDetails.price}}</view></view><view class="item"><view class="left">订单状态</view><view class="right">{{exemptionLabelOrderDetails.status}}</view></view><block wx:if="{{exemptionLabelOrderDetails.address}}"><view class="item"><view class="left">收货地址</view><view class="right">{{exemptionLabelOrderDetails.address}}</view></view></block><view class="item"><view class="left">订单号</view><view class="right">{{exemptionLabelOrderDetails.number}}</view></view><block wx:if="{{exemptionLabelOrderDetails.msg}}"><view class="item"><view class="left">备注信息</view><view class="right">{{exemptionLabelOrderDetails.msg}}</view></view></block><view class="item"><view class="left">下单时间</view><view class="right">{{exemptionLabelOrderDetails.time}}</view></view><block wx:if="{{exemptionLabelOrderDetails.express}}"><view class="item"><view class="left">快递公司</view><view class="right">{{exemptionLabelOrderDetails.express}}</view></view></block><block wx:if="{{exemptionLabelOrderDetails.courier_number}}"><view class="item"><view class="left">快递单号</view><view class="right">{{exemptionLabelOrderDetails.courier_number}}</view></view></block><block wx:if="{{exemptionLabelOrderDetails.deliver_time}}"><view class="item"><view class="left">发货时间</view><view class="right">{{exemptionLabelOrderDetails.deliver_time}}</view></view></block><block wx:if="{{exemptionLabelOrderDetails.refusetime}}"><view class="item"><view class="left">驳回时间</view><view class="right">{{exemptionLabelOrderDetails.refusetime}}</view></view></block><block wx:if="{{exemptionLabelOrderDetails.refuse}}"><view class="item"><view class="left">驳回原因</view><view class="right">{{exemptionLabelOrderDetails.refuse}}</view></view></block><block wx:if="{{exemptionLabelOrderDetails.status=='已发货'}}"><view class="delta err" hover-class="errs" data-event-opts="{{[['tap',[['complete',['$event']]]]]}}" bindtap="__e">确认签收</view></block><view class="delta" hover-class="deltas" data-event-opts="{{[['tap',[['delta',['$event']]]]]}}" bindtap="__e">返回上一页</view></view>